{
  "openapi": "3.0.0",
  "info": {
    "title": "QuickCall API",
    "version": "1.0.0",
    "description": ""
  },
  "servers": [
    {
      "url": "http://localhost:3000/"
    }
  ],
  "paths": {
    "/api/auth/register/": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "Register a new user",
        "description": "Endpoint to register a new user",
        "responses": {
          "default": {
            "description": ""
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterSchema"
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "RegisterSchema": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@example.com"
          },
          "password": {
            "type": "string",
            "example": "securePassword123"
          },
          "name": {
            "type": "string",
            "example": "John Doe"
          }
        }
      }
    }
  }
}